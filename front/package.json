{
  "name": "qwirk-phone",
  "version": "0.0.1",
  "description": "Qwirk phone application - Allow you to tchat with your friends and more with Qwirk-Sound that allow you to create your own song using your voice",
  "author": "Ravaniss <senechal.max06@gmail.com>",
  "license": "MIT",
  "scripts": {
    "android": "tns run android",
    "ios": "tns run ios",
    "dev": "tns debug android --no-hmr",
    "debug:android": "tns debug android",
    "debug:ios": "tns debug ios",
    "build:android": "tns build android --release",
    "build:ios": "tns build ios --release --for-device"
  },
  "nativescript": {
    "id": "com.kursorr.qwirk",
    "templateVersion": "v2",
    "tns-android": {
      "version": "6.5.0"
    },
    "tns-ios": {
      "version": "6.5.0"
    }
  },
  "dependencies": {
    "@nativescript/core": "^6.5.15",
    "@teammaestro/nativescript-svg": "^1.0.1",
    "@vue/devtools": "^5.3.3",
    "nativescript-socketio": "^3.3.1",
    "nativescript-toasty": "^3.0.0-alpha.2",
    "nativescript-vue": "^2.6.1",
    "nativescript-vue-devtools": "^1.4.0",
    "nativescript-vue-navigator": "^1.2.0",
    "nativescript-vue-shadow": "^0.1.0",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^9.0.0",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.1.0",
    "nativescript-dev-webpack": "^1.5.1",
    "nativescript-vue-template-compiler": "^2.6.0",
    "nativescript-worker-loader": "~0.11.0",
    "node-sass": "^4.13.1",
    "tns-platform-declarations": "^6.5.1",
    "typescript": "^3.8.3",
    "@types/node": "^13.11.1",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.1"
  }
